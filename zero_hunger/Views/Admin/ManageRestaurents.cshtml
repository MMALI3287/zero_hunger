@model IEnumerable<zero_hunger.EF.Restaurant>

@{
    ViewBag.Title = "ManageRestaurents";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>ManageRestaurents</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.location)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.supplier_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.contact_number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Registration.username)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {


<tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.location)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.supplier_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.contact_number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Registration.username)
        </td>
        <td>
            @Html.ActionLink("Edit", "ResEdit", new { id=item.id }) |
            @Html.ActionLink("Delete", "ResDelete", new { id=item.id })
        </td>
    </tr>
}

</table>
